<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background" >
        <shape>
            <!--<corners android:radius="10dip" />-->

            <stroke android:width="0.5dp"
                android:color="@color/app_color_231"/>
            <!--<padding android:left="1dp"-->
                <!--android:bottom="1dp"-->
                <!--android:right="1dp"-->
                <!--android:top="1dp"/>-->

            <gradient
                android:centerColor="@color/app_color_white"
                android:centerY="0.5"
                android:endColor="@color/app_color_white"
                android:startColor="@color/app_color_white"
                android:type="linear" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <!--<corners android:radius="10dip" />-->

                <gradient
                    android:centerColor="@color/app_color_231"
                    android:centerX="1.0"
                    android:endColor="@color/app_color_231"
                    android:startColor="@color/app_color_231" />
            </shape>
        </clip>
    </item>

</layer-list>
